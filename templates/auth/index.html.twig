{% extends 'base.html.twig' %}

{% block title %}
Keycloack Auth Demo Application
{% endblock %}

{% block body %}
    <h1>Authorization result</h1>

    {% if error is not null and error is not empty %}
        Error: {{ error }} <br>
    {% endif %}

    {% if userName is defined %}
        Username: {{ userName }}
    {% else %}
        Not authorized
    {% endif %}
{% endblock %}